<h1>Add a Contact:</h1>

<form action="/contact" method="post">
  <label for="first_name">First Name:</label>
  <input id="first_name" name="first_name" type="text">

  <label for="last_name">Last Name</label>
  <input id="last_name" name="last_name" type="text">

  <label for="job_title">Job:</label>
  <input id="job_title" name="job_title" type="text">

  <label for="company">Company:</label>
  <input id="company" name="company" type="text">

<%#
<label for="Phone">Phone Number:</label>
<input id="Phone" name="Phone" type="text">

  <label for="address">Address:</label>
  <input placeholder="street" id="street" name="address" type="text">
  <input placeholder="city" id="city" name="address" type="text">
  <input placeholder="state" id="state" name="address" type="text">
  <input placeholder="zip" id="zip" name="address" type="number">


  <label for="email">Email:</label>
  <input placeholder="email@address.com" id="email" name="company" type="text">
  %>

  <button type="submit">Add Contact</button>
</form>
